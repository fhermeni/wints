{{#if this}}
<h3>{{len this}} internships
<button type="button" class="btn" onclick="mailing('tutor')">
    <i class="glyphicon glyphicon-envelope"></i>
    mail selection
</button>
<button type="button" class="btn" onclick="showRawFullname()">
    <i class="glyphicon glyphicon-list"></i>
    selection fullname
</button>
</h3>
<table class="table table-condensed table-hover tablesorter" id="table-conventions">
    <thead>
    <tr>
        <th class="sorter-false">
        <input type='checkbox' data-toggle='checkbox' class='icheckbox_flat check_all'/>
        </th>
        <th>Student</th>
        <th>Promotion</th>
        <th>Major</th>
        <th class="sorter-false">Supervisor</th>
        <th>Tutor</th>
        <th class="sorter-false">Reports</th>
    </tr>
    </thead>
    <tbody>
    {{#each this}}
        <tr>
            <td>
            <input type='checkbox' class='icheckbox_flat' data-email='{{Student.Email}}'/>  
            </td>
            <td>
            <a class='clickable' onclick="showDetails('{{Stu.P.Email}}')">{{fullname Student}}</a>
            </td>
            <td>{{Promotion}}</td>
            <td>{{Major}}</td>
            <td>                
                <a href='mailto:{{Sup.Email}}' title='{{fullname Sup}}'>{{fullname Sup}}</a>
            </td>
            <td>                
                <a href='mailto:{{Tutor.Email}}' title='{{fullname Tutor}}'>{{fullname Tutor}}</a>
            </td>            
            <td>
                {{#each Reports}}
                <span>
                {{reportHeader this}}
                </span>
                {{/each}}
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>
{{else}}
    <h5 class="text-center">No internships to watch</h5>
{{/if}}