<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <h3 class="modal-title">Supervisor Editor</h3>
        </div>
        <div class="modal-body">
            <div class="form-horizontal">
                <div class="form-group">
                    <label for="lbl-fn" class="col-sm-3 control-label">Firstname</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" id="lbl-fn" value="{{Sup.Firstname}}"/>
                        </div>
                </div>
                <div class="form-group">
                    <label for="lbl-ln" class="col-sm-3 control-label">Lastname</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" id="lbl-ln" value="{{Sup.Lastname}}"/>
                        </div>
                </div>
                <div class="form-group">
                    <label for="lbl-www" class="col-sm-3 control-label">Email</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" id="lbl-email" value="{{Sup.Email}}"/>
                        </div>
                </div>                
                <div class="form-group">
                    <label for="lbl-tel" class="col-sm-3 control-label">Telephon</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" id="lbl-tel" value="{{Sup.Tel}}"/>
                        </div>
                </div>                
                <div class="modal-footer">
                    <div class="text-right form-group">
                        <button type="button" class="btn btn-success" onclick="sendSupervisor()">Update</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>    